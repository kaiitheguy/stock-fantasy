{
  "expo": {
    "name": "stock-fantasy",
    "slug": "stock-fantasy",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.halfnoir.stockfantasy",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      },
      "extra": {
        "EXPO_PUBLIC_API_BASE": "http://0.0.0.0:3000"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "extra": {
        "EXPO_PUBLIC_API_BASE": "http://0.0.0.0:3000"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "f56c30c1-69dd-440a-8b64-3a76e97d2593"
      }
    }
  }
}
